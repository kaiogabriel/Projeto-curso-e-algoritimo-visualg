algoritmo "operadores relacionais"
// Função :
// Autor :
// Data : 26/04/2020
// Seção de Declarações 
var
   nota01, nota02:real
inicio
// Seção de Comandos
   nota01 :=10
   nota02 :=15
   
   se(nota01 <> nota02) entao
        escreval ("nota01 é diferente")
   senao
        escreval ("nota01 é igual")
   fimse
fimalgoritmo
